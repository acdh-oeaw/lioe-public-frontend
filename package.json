{
  "name": "@acdh-oeaw/lioe-frontend",
  "private": true,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "packageManager": "npm@9.6.7",
  "scripts": {
    "build": "run-s build:client build:server",
    "build:client": "dotenv -c production -- vite build",
    "build:server": "dotenv -c production -- tsup-node ./server/index.ts --format esm --out-dir ./dist/server/ --target es2021",
    "dev": "run-p dev:client dev:server",
    "dev:client": "dotenv -c development -- vite dev",
    "dev:server": "dotenv -c development -- tsx watch ./server/index.ts",
    "start": "dotenv -c production -- node ./dist/server/index.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui"
  },
  "dependencies": {
    "@acdh-oeaw/lib": "^0.1.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@mdi/font": "^5.3.45",
    "axios": "^0.21.4",
    "bootstrap-vue": "^2.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "dom-to-image": "^2.6.0",
    "express": "^4.18.2",
    "file-saver": "^1.3.8",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "uuid": "^3.4.0",
    "vue": "^2.7.14",
    "vue-class-component": "^7.2.6",
    "vue-clipboard2": "^0.3.1",
    "vue-horizontal": "^0.8.12",
    "vue-lazyload": "^1.3.3",
    "vue-matomo": "^4.2.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.6.5",
    "vue-tour": "^2.0.0",
    "vue2-leaflet": "^2.7.1",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.7.1",
    "vuewordcloud": "^18.7.12",
    "vuex": "^3.6.2",
    "vuex-class-component": "^2.3.6",
    "xlsx": "^0.17.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@playwright/test": "^1.40.1",
