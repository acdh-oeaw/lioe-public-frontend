    "@types/cors": "^2.8.14",
    "@types/dom-to-image": "^2.6.0",
    "@types/express": "^4.17.17",
    "@types/file-saver": "^1.3.1",
    "@types/geojson": "^7946.0.7",
    "@types/leaflet": "^1.9.4",
    "@types/lodash": "^4.14.152",
    "@types/morgan": "^1.9.9",
    "@types/node": "^18.17.15",
    "@vitejs/plugin-vue2": "^2.2.0",
    "ci-info": "^4.0.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "dotenv-expand": "^10.0.0",
    "npm-run-all2": "^6.0.6",
    "sass": "^1.67.0",
    "start-server-and-test": "^1.13.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.10",
